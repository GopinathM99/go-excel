{
  "name": "@excel/core",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./models": {
      "types": "./src/models/index.ts",
      "import": "./src/models/index.ts"
    },
    "./formula": {
      "types": "./src/formula/index.ts",
      "import": "./src/formula/index.ts"
    },
    "./collab": {
      "types": "./src/collab/index.ts",
      "import": "./src/collab/index.ts"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:perf": "vitest run --config vitest.perf.config.ts",
    "test:perf:watch": "vitest --config vitest.perf.config.ts",
    "perf:validate": "npx tsx scripts/validate-performance.ts",
    "perf:report": "npx tsx scripts/generate-perf-report.ts",
    "perf:baseline": "npx tsx scripts/validate-performance.ts --update-baseline",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@excel/shared": "workspace:*",
    "exceljs": "^4.4.0",
    "lib0": "^0.2.98",
    "y-protocols": "^1.0.6",
    "y-websocket": "^2.0.4",
    "yjs": "^13.6.18"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.2.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
