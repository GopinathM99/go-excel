{
  "name": "@excel/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:perf": "vitest run tests/perf/",
    "test:perf:watch": "vitest tests/perf/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen http://localhost:3000",
    "test:compat": "playwright test --config=playwright.compat.config.ts",
    "test:compat:chrome": "playwright test --config=playwright.compat.config.ts --project=chromium",
    "test:compat:firefox": "playwright test --config=playwright.compat.config.ts --project=firefox",
    "test:compat:safari": "playwright test --config=playwright.compat.config.ts --project=webkit",
    "test:compat:edge": "playwright test --config=playwright.compat.config.ts --project=edge",
    "test:compat:mobile": "playwright test --config=playwright.compat.config.ts --project=mobile-chrome --project=mobile-safari",
    "test:compat:ui": "playwright test --config=playwright.compat.config.ts --ui",
    "test:compat:debug": "playwright test --config=playwright.compat.config.ts --debug",
    "test:compat:update-snapshots": "playwright test --config=playwright.compat.config.ts --update-snapshots",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@excel/core": "workspace:*",
    "@excel/shared": "workspace:*",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.2.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  }
}
